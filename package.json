{
  "name": "learning_knexjs",
  "version": "1.0.0",
  "description": "Project to lean more about Knex.js lib",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "migrations:dev": "knex migrate:latest --knexfile knexfile.ts",
    "migrations:dev:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migration:add": "knex migrate:make --knexfile knexfile.ts -x ts",
    "seed:add": "knex seed:make --knexfile knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felipegueller/learning_knexjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/felipegueller/learning_knexjs/issues"
  },
  "homepage": "https://github.com/felipegueller/learning_knexjs#readme",
  "dependencies": {
    "@types/express": "^4.17.14",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "knex": "^2.3.0",
    "pg": "^8.8.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  }
}
